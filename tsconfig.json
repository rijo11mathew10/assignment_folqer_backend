{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules",
    "dist",
    "test",
    "tests",
  ],
}